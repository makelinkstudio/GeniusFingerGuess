<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/cq_prompt_bg"
    android:gravity="center"
    android:orientation="vertical">
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center"
        android:paddingTop="10dp"
       android:paddingBottom="10dp" 
        android:paddingLeft="30dp"
    	android:paddingRight="30dp" >
    <TextView 
        android:id="@+id/tvw_mat_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textSize="@dimen/text_large"
        android:text="9:00"/>
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="horizontal">
         <TextView 
             android:id="@+id/tvw_win_camp"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
		    android:layout_weight="1"
		    android:gravity="center"
		    android:text="@string/cq_zhenxing_A"
		    android:textSize="@dimen/text_large"
		    android:textColor="@color/cq_white_80"/>
           <TextView 
		     android:layout_width="0dp"
		     android:layout_height="wrap_content"
		     android:layout_weight="1"
		     android:gravity="center"
		     android:textSize="@dimen/text_large"
		     android:textColor="@color/white"/>
          <TextView 
             android:id="@+id/tvw_lose_camp"
		     android:layout_width="0dp"
		     android:layout_height="wrap_content"
		     android:layout_weight="1"
		     android:gravity="center"
		     android:text="@string/cq_zhenxing_B"
		     android:textSize="@dimen/text_large"
		     android:textColor="@color/cq_white_80"/>
    </LinearLayout>
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center_vertical"
        android:orientation="horizontal">
        <ImageView 
            android:id="@+id/ivw_win_punch"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:src="@drawable/jian_right"/>
         <TextView 
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
		    android:layout_weight="1"
		    android:gravity="center"
		    android:text="K.O."
		    android:textSize="@dimen/text_xlarge"
		     android:textColor="@color/cq_A_color"
		     android:textStyle="italic"/>
          <ImageView 
            android:id="@+id/ivw_lose_punch"  
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:src="@drawable/bao_left"/>
    </LinearLayout>
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center_vertical"
        android:orientation="horizontal">
           <TextView 
            android:id="@+id/tvw_win_integral"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
		    android:layout_weight="1"
		    android:gravity="center"
		    android:text="20000"
		    android:textStyle="italic"
		    android:textSize="@dimen/text_large"
			android:textColor="@color/cq_B_color"/>
		  <TextView 
			android:id="@+id/tvw_you_result"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
		    android:layout_weight="1"
		    android:gravity="center"
		    android:text="YOU WIN"
		    android:textStyle="italic|bold"
		    android:textSize="@dimen/text_large"
			android:textColor="@color/cq_A_color"/>
		    <TextView 
		     android:id="@+id/tvw_lose_integral"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
		    android:layout_weight="1"
		    android:gravity="center"
		    android:text="2300"
		    android:textStyle="italic"
		    android:textSize="@dimen/text_large"
			android:textColor="@color/cq_Shi_color"/>
	   </LinearLayout>
	  <TextView 
	    android:id="@+id/tvw_add_integral"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginTop="5dp"
	    android:text="0"
	    android:textSize="@dimen/text_large"
		android:textColor="@color/cq_A_color"/>
	 </LinearLayout>
	  <LinearLayout 
	       android:layout_width="match_parent"
	       android:layout_height="0.5dp"
	       android:background="#7B7B7B">
	   </LinearLayout>
	  <LinearLayout 
	      android:layout_width="match_parent"
	      android:layout_height="wrap_content"
	      android:orientation="horizontal">
	      <TextView 
	          android:id="@+id/tvw_comfirm"
	         android:layout_width="0dp" 
	         android:layout_height="wrap_content"
	         android:layout_weight="1"
	         android:paddingTop="15dp"
	         android:paddingBottom="15dp"
	         android:gravity="center_horizontal"
	         android:textColor="@color/white"
	         android:textSize="@dimen/text_large"
	         android:text="知道了"/>
	       <LinearLayout 
		       android:layout_width="0.5dp"
		       android:layout_height="match_parent"
		       android:layout_weight="0"
		       android:background="#7B7B7B">
	   		</LinearLayout>
	     <TextView 
	          android:id="@+id/tvw_share"
	         android:layout_width="0dp" 
	         android:layout_height="wrap_content"
	         android:layout_weight="1"
	         android:paddingTop="15dp"
	         android:paddingBottom="15dp"
	         android:gravity="center_horizontal"
	         android:textColor="@color/white"
	         android:textSize="@dimen/text_large"
	         android:text="立即分享"/>
	  </LinearLayout>
</LinearLayout>
